+    const largeComponent = await mount(
+      renderWithProviders(<Input size="lg" />)
+    );
+    await expect(largeComponent.locator('input')).toHaveClass(/h-12/);
+    await expect(largeComponent.locator('input')).toHaveClass(/px-6/);
